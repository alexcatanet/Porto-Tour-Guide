<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!-- Separator line-->
    <style name="Divider">
        <item name="android:layout_width">333dp</item>
        <item name="android:layout_marginStart">11dp</item>
        <item name="android:layout_height">0.1dp</item>
        <item name="android:layout_below">@id/text_name_object</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:background">#767575</item>
    </style>

    <style name="SubmitButtonStyle">
        <item name="android:layout_width">320dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>


    <style name="IntroPage" parent="Theme.AppCompat.NoActionBar">

    </style>

</resources>
